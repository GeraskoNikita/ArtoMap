<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_map">

    <fragment
        android:id="@+id/navigation_map"
        android:name="com.example.artomap.ui.map.MapFragment"
        android:label="@string/Map"
        tools:layout="@layout/fragment_map" />

    <fragment
        android:id="@+id/navigation_data"
        android:name="com.example.artomap.ui.database.DataFragment"
        android:label="@string/DataBase"
        tools:layout="@layout/fragment_data" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.example.artomap.ui.settings.SettingsFragment"
        android:label="@string/Settings"
        tools:layout="@layout/fragment_settings" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.artomap.ui.profile.ProfileFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_profile" />
</navigation>